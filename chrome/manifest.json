{
  "manifest_version": 2,

  "name": "helpers",
  "description": "",
  "version": "1.0",

  "background": {
    "scripts": [ "background.js" ]
  },

  "browser_action": {
    //"default_popup": "capture.html",
    "default_icon": "icon.svg"
  },

  "permissions": [
    "desktopCapture"
  ],
  
  "content_scripts": [
    {
      "matches": [ "http://*/*" ],
      "js": [ "inject.js" ]
    }
  ]
}